{
  "Box comment": {
      "prefix": ["boxcom"],
      "body": [
          "////////////////////////////////////////////////////////////////////////////////",
          "// ${1:$TM_SELECTED_TEXT}",
          "////////////////////////////////////////////////////////////////////////////////"
      ]
  },

  "JSON console log": {
      "prefix": ["clogj"],
      "body": [
          "console.log(JSON.stringify(${0:$TM_SELECTED_TEXT}, null, 2))"
      ]
  },

  "console log": {
      "prefix": ["clog"],
      "body": [
          "console.log(`${1:$TM_SELECTED_TEXT} - \\${${1:$TM_SELECTED_TEXT}\\}`)$0"
      ]
  },

  "Import": {
      "prefix": ["imp"],
      "body": [
          "import {$0} from '$1'"
      ]
  },

  "Export": {
      "prefix": ["exp"],
      "body": [
          "export {$0} from '$1'"
      ]
  }
}
